{% macro examplesSection(dataArr, title) %}
<section class="ivideon-examples">
    <div class="iv-container">
        <h2 class="iv-h2-base ivideon-examples__title">{{title}}</h2>
        {% from "app/njk/components/ivideon_examples_item.njk" import examplesItem %}
        <div class="ivideon-examples__slider js--examples-slider">
            {% for item in dataArr %}
            <div class="ivideon-examples__slide-wrapper">
                {{ examplesItem(item.img, item.title, item.text) }}
            </div>
            {% endfor %}
        </div>
    </div>
</section>
{% endmacro %}