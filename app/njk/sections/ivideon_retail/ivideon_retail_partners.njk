            {% set partnersItem = [
            "../../images/dest/img/ivideon_retail/retail_partners1.png",
            "../../images/dest/img/ivideon_retail/retail_partners2.png",
            "../../images/dest/img/ivideon_retail/retail_partners3.png",
            "../../images/dest/img/ivideon_retail/retail_partners4.png",
            "../../images/dest/img/ivideon_retail/retail_partners5.png",
