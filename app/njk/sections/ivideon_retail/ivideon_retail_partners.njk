            "../../images/dest/img/ivideon_retail/retail_partners8.png",
            "../../images/dest/img/ivideon_retail/retail_partners9.png",
            "../../images/dest/img/ivideon_retail/retail_partners10.png",
            "../../images/dest/img/ivideon_retail/retail_partners11.png",
            "../../images/dest/img/ivideon_retail/retail_partners12.png",
            "../../images/dest/img/ivideon_retail/retail_partners13.png",
            "../../images/dest/img/ivideon_retail/retail_partners14.png",
            "../../images/dest/img/ivideon_retail/retail_partners15.png",
            "../../images/dest/img/ivideon_retail/retail_partners16.png"
            ]%}
            {% for item in partnersItem %}
            <div class="ivideon-retail-partners__col">
                <div class="ivideon-retail-partners__item">
                    <img src="{{item}}" alt="партнеры">
                </div>
            </div>
            {% endfor %}
